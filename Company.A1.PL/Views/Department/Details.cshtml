@model Department

@{
    ViewData["Title"] = "Details";
}

<h1>Details Page</h1>
<br />
<br />

<div class="my-3">
    <label asp-for="Code" class="col-form-label"></label>
    <input asp-for="Code" class="form-control" readonly />
</div>
<div class="my-3">
    <label asp-for="Name" class="col-form-label"></label>
    <input asp-for="Name" class="form-control" readonly />
</div>
<div class="my-3">
    <label asp-for="CreateAt" class="col-form-label"></label>
    <input asp-for="CreateAt" class="form-control" readonly />

</div>

<a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
<a asp-action="Index" class="btn btn-light">Back To List</a>